
public class CircumferenceFinder {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
final float PI = 3.14f;

float radius;
float circumference;

System.out.println("Enter the radius: ");
radius = input.nextLine();

circumference = 2 * radius * PI;
	}

}
