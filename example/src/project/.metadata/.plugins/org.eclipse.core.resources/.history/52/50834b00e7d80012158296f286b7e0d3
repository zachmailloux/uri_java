
public class CatClass {
	/**
	 * @param args
	 */

		// variables
		private int age;
		private int tail;
		private String name;
		
		//constructors
		CatClass(){
			age = 2;
			tail = 12;
			name = "Cat";}
		
		//constructors
		CatClass(int a , int t , String n){
			age = a;
			tail = t;
			name = n;
		}
		
		//Setters
		public void setAge(int a){
			age = a;
			
		}
		
		//Getters
		public int getAge(){
			return age;
			
		}		
		
		//Methods
		public void ageMethod(int a){
			age = age + a;
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
	}


